void swap(int[] arr, int i, int j)
{
    int temp = arr[i];
    arr[i] = arr[j];
    arr[j] = temp;
}

void bubbleSort(int[] arr, int n)
{
    int i = 0;
    int j = 0;
    while (i < n-1)
    {
        j = 0;
        while (j < n-i-1){
            if (arr[j] > arr[j+1]) {
                swap(arr, j, j+1);
            }
            j = j + 1;
        }
        i = i + 1;
    }
}

