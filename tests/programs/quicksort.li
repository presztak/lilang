void quicksort(int[] arr, int first, int last) {
   int i = 0;
   int j = 0;
   int pivot = 0;
   int temp = 0;

   if(first < last) {
      pivot = first;
      i = first;
      j = last;

      while(i < j) {
         while(arr[i] <= arr[pivot] && i < last) {
             i = i + 1;
         }
         while(arr[j] > arr[pivot]) {
             j = j - 1;
         }

         if(i<j) {
            temp = arr[i];
            arr[i] = arr[j];
            arr[j] = temp;
         }
      }

      temp = arr[pivot];
      arr[pivot] = arr[j];
      arr[j] = temp;
      quicksort(arr, first, j - 1);
      quicksort(arr, j + 1, last);
   }
}

